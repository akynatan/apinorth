    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
