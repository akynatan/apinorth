  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "mrun": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts migration:run",
    "mrevert": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts migration:revert",
    "drop_leads": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts query 'drop table leads'",
    "orm": "yarn typeorm -d ./src/shared/infra/typeorm/index.ts "
  },
  "dependencies": {
